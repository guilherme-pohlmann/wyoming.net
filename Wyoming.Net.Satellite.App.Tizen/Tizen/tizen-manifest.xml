<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns="http://tizen.org/ns/packages" api-version="8.0" package="org.tizen.example.Tizen" version="1.0.0">
  <profile name="tv" />
  <ui-application appid="org.tizen.example.Tizen"
					exec="Wyoming.Net.Satellite.App.Tizen.dll"
					type="dotnet-nui"
					multiple="false"
					taskmanage="true"
					nodisplay="false"
					launch_mode="single"
                    api-version="12">
    <label>Tizen</label>
    <icon>Tizen.png</icon>
    <metadata key="http://tizen.org/metadata/prefer_dotnet_aot" value="true" />
  </ui-application>
   <privileges> 
         <!--To access media storage-->
        <privilege>http://tizen.org/privilege/mediastorage</privilege>  

        <!--To access, read, and write to the external storage-->
        <privilege>http://tizen.org/privilege/externalstorage</privilege>
        <privilege>http://tizen.org/privilege/ml.inference</privilege>
        <privilege>http://tizen.org/privilege/recorder</privilege>
        <privilege>http://tizen.org/privilege/audio.output</privilege>
     </privileges>
    <feature name="http://tizen.org/feature/machine_learning"/>
    <feature name="http://tizen.org/feature/machine_learning.inference"/>
    <feature name="http://tizen.org/feature/microphone"/>
</manifest>
